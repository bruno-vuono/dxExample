    <validationRules>
        <fullName>ASI_CRM_MY_FIS_BlankChk</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
$User.BypassVR__c != TRUE,
CONTAINS($RecordType.DeveloperName, &apos;ASI_FOC_MY_&apos;), 
OR( 
ISBLANK(ASI_FOC_SKU__c), 
ISBLANK(ASI_FOC_Issued_Qty__c), 
ISBLANK(ASI_CRM_SetupCode__c) 
) 
)</errorConditionFormula>
        <errorMessage>The following fields must be filled: 
- SKU, Issued Qty, Setup Code</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_SG_Adjustment_Remarks</fullName>
        <active>false</active>
        <errorConditionFormula>AND (
   RecordType.DeveloperName = &apos;ASI_SG_CRM_Request_Items&apos;,
   ASI_FOC_Actual_Quantity_Bottle__c &lt;&gt;  ASI_FOC_Request_Quantity_Bottle__c ,
   OR (ISBLANK(ASI_CRM_Adjustment_Remarks__c ),
 LEN(ASI_CRM_Adjustment_Remarks__c )&lt; 8 
)
)</errorConditionFormula>
        <errorDisplayField>ASI_CRM_Adjustment_Remarks__c</errorDisplayField>
        <errorMessage>Please input remarks of this adjustment with minimum of 8 letters.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_FOC_CN_POSM_Check_Available_Quantity</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
 CONTAINS( RecordType.DeveloperName , &quot;ASI_FOC_CN_POSM_Request_Item&quot;),
NOT($User.BypassVR__c),  
OR( 
AND ( 
NOT(ISNEW()), 
ASI_FOC_Request_Order__r.ASI_FOC_SYS_Number_of_Saves__c &gt; 0, 
priorvalue(ASI_FOC_Request_Quantity_Bottle__c) &lt;&gt; ASI_FOC_Request_Quantity_PC__c, 
ASI_FOC_Request_Quantity_PC__c &gt; ASI_FOC_SKU__r.ASI_HK_CRM_Available_Quantity__c
) 
, 
AND ( 
NOT(ISNEW()), 
ASI_FOC_Request_Order__r.ASI_FOC_SYS_Number_of_Saves__c = 0, 
ASI_FOC_Request_Quantity_PC__c &gt; ASI_FOC_SKU__r.ASI_HK_CRM_Available_Quantity__c
) 
, 
AND ( 
ISNEW(), 
ASI_FOC_Request_Quantity_PC__c &gt; ASI_FOC_SKU__r.ASI_HK_CRM_Available_Quantity__c
) 
) 
)</errorConditionFormula>
        <errorDisplayField>ASI_FOC_Original_Request_Quantity_PC_BT__c</errorDisplayField>
        <errorMessage>SKU lack of stock</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_FOC_CN_QuantityValidation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
OR(CONTAINS( RecordType.DeveloperName , &quot;ASI_FOC_CN_POSM_Request_Item&quot;), CONTAINS( RecordType.DeveloperName , &quot;ASI_FOC_CN_Request_Item&quot;)),
ASI_FOC_Original_Request_Quantity_PC_BT__c &lt; 1
)</errorConditionFormula>
        <errorDisplayField>ASI_FOC_Original_Request_Quantity_PC_BT__c</errorDisplayField>
        <errorMessage>Quantity must be greater than 0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_FOC_REDUCE_QUANTITY</fullName>
        <active>true</active>
        <description>when Rejected by Logistic</description>
        <errorConditionFormula>AND(
CONTAINS($RecordType.DeveloperName, &apos;ASI_FOC_CN_&apos;), 
AND(
ISPICKVAL( ASI_FOC_Request_Order__r.ASI_FOC_Request_Status__c, &quot;Rejected by Logistic&quot;),
ISCHANGED( ASI_FOC_Request_Quantity_Bottle__c ),
ASI_FOC_Request_Quantity_Bottle__c &gt; PRIORVALUE( ASI_FOC_Request_Quantity_Bottle__c )
)
)</errorConditionFormula>
        <errorDisplayField>ASI_FOC_Request_Quantity_Bottle__c</errorDisplayField>
        <errorMessage>The new quantity cannot be greater than the previous quantity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_FOC_Should_not_exceed_Inventory</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
 CONTAINS( RecordType.DeveloperName , &quot;ASI_FOC_CN_Request_Item&quot;),
NOT($User.BypassVR__c), 
NOT(ISCHANGED( ASI_FOC_SO_Actual_Extended_Cost__c )), 
NOT(ISCHANGED( ASI_FOC_Free_Goods_Actual_Cost__c )), 
NOT(ISCHANGED( ASI_FOC_Actual_Quantity_Bottle__c )), 
OR( 
AND ( 
NOT(ISNEW()), 
ASI_FOC_Request_Order__r.ASI_FOC_SYS_Number_of_Saves__c &gt; 0, 
priorvalue(ASI_FOC_Original_Request_Quantity_PC_BT__c) &lt;&gt; ASI_FOC_Original_Request_Quantity_PC_BT__c, 
ASI_FOC_Original_Request_Quantity_PC_BT__c &gt; ASI_FOC_SKU__r.ASI_FOC_Inventory__c 
) 
, 
AND ( 
NOT(ISNEW()), 
ASI_FOC_Request_Order__r.ASI_FOC_SYS_Number_of_Saves__c = 0, 
ASI_FOC_Original_Request_Quantity_PC_BT__c &gt; ASI_FOC_SKU__r.ASI_FOC_Inventory__c 
) 
, 
AND ( 
ISNEW(), 
ASI_FOC_Original_Request_Quantity_PC_BT__c &gt; ASI_FOC_SKU__r.ASI_FOC_Inventory__c 
) 
) 
)</errorConditionFormula>
        <errorDisplayField>ASI_FOC_Original_Request_Quantity_PC_BT__c</errorDisplayField>
        <errorMessage>SKU lack of stock</errorMessage>
    </validationRules>
