    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>ASI_CRM_CN_PSF_Period_Checking</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ASI_CRM_CN_Period_Start__c &gt;  ASI_CRM_CN_Period_End__c ,  RecordType.DeveloperName =&apos;ASI_CRM_CN_Payment_Request_Detail_PSF&apos;)</errorConditionFormula>
        <errorMessage>Please select correct PSF period</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_CN_Validate_PSF_Period</fullName>
        <active>true</active>
        <errorConditionFormula>ASI_CRM_CN_Period_Start__c  &gt; ASI_CRM_CN_Period_End__c  ||
 ASI_CRM_CN_Period_Start__c   &lt;  ASI_TH_CRM_Payment_Request__r.ASI_CRM_CN_PO_Start_Date__c ||
ASI_CRM_CN_Period_End__c  &gt; ASI_TH_CRM_Payment_Request__r.ASI_CRM_CN_PO_End_Date__c</errorConditionFormula>
        <errorDisplayField>ASI_CRM_CN_AC_Code__c</errorDisplayField>
        <errorMessage>PSF period must be within PO Date Range</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_MY_PRLine_BlankChk</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
$User.BypassVR__c != TRUE,
CONTAINS($RecordType.DeveloperName, &apos;ASI_CRM_MY&apos;), 
OR(
ISBLANK(ASI_CRM_SetupCode__c),
ISBLANK(ASI_CRM_SKU__c),
ISBLANK(ASI_CRM_CN_Actual_Vol__c),
ISBLANK(ASI_TH_CRM_Amount__c)
) 
)</errorConditionFormula>
        <errorMessage>The following fields must be filled: 
- Setup Code, SKU, Actual Vol (Std. Bottle), Amount</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_SG_BottleIncentive_BlockNegative</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
  RecordType.DeveloperName = &apos;ASI_CRM_SG_Payment_Request_Detail&apos;,
  ASI_TH_CRM_Payment_Request__r.ASI_CRM_System_Generated__c,
  ISPICKVAL(ASI_CRM_SG_Type__c , &apos;Bottle Incentive&apos;),
  OR (
    ISBLANK(ASI_CRM_Adjusted_Amount__c) ,
    ASI_CRM_Adjusted_Amount__c &lt; 0
  )
)</errorConditionFormula>
        <errorMessage>Adjusted Amount must be greater than or equal to $0.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_SG_BottleIncentive_Remarks</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
  RecordType.DeveloperName = &apos;ASI_CRM_SG_Payment_Request_Detail&apos;,
  ASI_TH_CRM_Payment_Request__r.ASI_CRM_System_Generated__c,
  ISPICKVAL(ASI_CRM_SG_Type__c , &apos;Bottle Incentive&apos;),
  ASI_TH_CRM_Amount__c != ASI_CRM_Adjusted_Amount__c,
  ISBLANK(ASI_CRM_Adjustment_Remark__c) 
)</errorConditionFormula>
        <errorMessage>Please input Remark if you have adjustment.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_SG_PaymentCOALocalChannelCheck</fullName>
        <active>false</active>
        <errorConditionFormula>AND (
 CONTAINS( RecordType.DeveloperName , &quot;ASI_CRM_SG&quot;),
 ISBLANK( TEXT(ASI_TH_CRM_Payment_Request__r.ASI_CRM_CN_OutletWS__r.ASI_CRM_SG_Local_Channel__c)),
 ASI_CRM_SG_JDE_Is_Append_Local_Channel__c 
)</errorConditionFormula>
        <errorMessage>The Outlet of this payment need to have Local Channel for this expense type.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_SG_WSBackRebate_BlockGiveMore</fullName>
        <active>false</active>
        <errorConditionFormula>AND (
  RecordType.DeveloperName = &apos;ASI_CRM_SG_Payment_Request_Detail&apos;,
  ASI_TH_CRM_Payment_Request__r.ASI_CRM_System_Generated__c, 
  !ISBLANK(ASI_TH_CRM_Payment_Request__r.ASI_TH_CRM_Contract__c),
  ISPICKVAL(ASI_CRM_SG_Type__c , &apos;Back Rebates&apos;),
  ASI_TH_CRM_Amount__c &lt; ASI_CRM_Adjusted_Amount__c
)</errorConditionFormula>
        <errorMessage>Adjusted Amount must be less than or equal to the computed amount.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_SG_WSBackRebate_BlockNegative</fullName>
        <active>false</active>
        <errorConditionFormula>AND (
  RecordType.DeveloperName = &apos;ASI_CRM_SG_Payment_Request_Detail&apos;,
  !ISBLANK(ASI_TH_CRM_Payment_Request__r.ASI_TH_CRM_Contract__c),
  ISPICKVAL(ASI_CRM_SG_Type__c , &apos;Back Rebates&apos;),
  OR (
    ISBLANK(ASI_CRM_Adjusted_Amount__c) ,
    ASI_CRM_Adjusted_Amount__c &lt; 0
  )
)</errorConditionFormula>
        <errorMessage>Adjusted Amount must be greater than or equal to $0.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_SG_WSBackRebate_Remarks</fullName>
        <active>false</active>
        <errorConditionFormula>AND (
  RecordType.DeveloperName = &apos;ASI_CRM_SG_Payment_Request_Detail&apos;,
  ASI_TH_CRM_Payment_Request__r.ASI_CRM_System_Generated__c, 
  !ISBLANK(ASI_TH_CRM_Payment_Request__r.ASI_TH_CRM_Contract__c),
  ISPICKVAL(ASI_CRM_SG_Type__c , &apos;Back Rebates&apos;),
  ASI_TH_CRM_Amount__c != ASI_CRM_Adjusted_Amount__c,
  ISBLANK(ASI_CRM_Adjustment_Remark__c) 
)</errorConditionFormula>
        <errorMessage>Please input Remark if you have adjustment.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_TH_CRM_ValidateUnitCost</fullName>
        <active>true</active>
        <errorConditionFormula>AND(AND(TEXT(ASI_TH_CRM_COA__c)&lt;&gt;&apos;Fund - FOC&apos;,TEXT(ASI_TH_CRM_COA__c)&lt;&gt;&apos;Activity - FOC&apos;), OR(ISBLANK(ASI_TH_CRM_UnitCost__c) ,ISNULL(ASI_TH_CRM_UnitCost__c)), RecordType.DeveloperName=&apos;ASI_TH_CRM_Payment_Request_Detail&apos; )</errorConditionFormula>
        <errorDisplayField>ASI_TH_CRM_UnitCost__c</errorDisplayField>
        <errorMessage>Please enter Unit Cost.</errorMessage>
    </validationRules>
