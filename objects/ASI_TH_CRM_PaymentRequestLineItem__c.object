    <fields>
        <fullName>ASI_CRM_SG_JDE_COA_Code__c</fullName>
        <description>First half: Construct COA based on Payment Request Type
Second half: certain COA requires appending Category Code 13 (Sector Code) in the COA
Requirement to allow user select IIO/HNR on Outlet, but JDE COA map to HR

20170223 CHM114259479: Default IIO/HNR mapping to _HNR
20180601: Back Rebate no need Cat Code 13</description>
        <externalId>false</externalId>
        <formula>CASE( TEXT(ASI_CRM_SG_Type__c  ) ,
	&quot;A&amp;P Fund&quot;, &quot;5275.ADH_&quot;, 
	&quot;Ad-hoc Promotions&quot;, &quot;5275.ADH_&quot;, 
	&quot;Cash - Target Achieved&quot;, &quot;5275.CASH_&quot;,
	&quot;Cash - Upfront&quot;, &quot;1430.CS&quot;,
	&quot;Contract Margin&quot;, &quot;5245.MARG_&quot;,
	 &quot;Events&quot;, &quot;5558.SPON&quot;, 
	 &quot;Product Branding Display&quot;, &quot;5556&quot;, 
	 &quot;Trade Incentive Trips&quot;, &quot;5553.TRDINC&quot;, 
	 &quot;Bottle Incentive&quot;, &quot;5275.CEMP_&quot;, 
	 &quot;Back Rebates&quot;, &quot;5275.CEMP&quot;, 
	 &quot;Sales Support&quot;, &quot;5556&quot;, 
	 &quot;Signages&quot;, &quot;5280.SUPP_&quot;, 
	 &quot;Advertising Spot&quot;, &quot;5582&quot;, 
	 &quot;ERROR&quot;
 ) 
 + 
 IF( 
	IF (
		OR (TEXT(ASI_CRM_SG_Type__c)=&quot;A&amp;P Fund&quot;, 
		TEXT(ASI_CRM_SG_Type__c)=&quot;Ad-hoc Promotions&quot;, 
		TEXT(ASI_CRM_SG_Type__c)=&quot;Cash - Target Achieved&quot;, 
		TEXT(ASI_CRM_SG_Type__c)=&quot;Contract Margin&quot;, 
		TEXT(ASI_CRM_SG_Type__c)=&quot;Bottle Incentive&quot;, 
		TEXT(ASI_CRM_SG_Type__c)=&quot;Signages&quot;
		), 
		true,
		false
	),  
	IF(
		OR(TEXT(ASI_TH_CRM_Payment_Request__r.ASI_CRM_CN_OutletWS__r.ASI_CRM_SG_Category_Code_13__c)=&quot;HNR&quot;, TEXT(ASI_TH_CRM_Payment_Request__r.ASI_CRM_CN_OutletWS__r.ASI_CRM_SG_Category_Code_13__c)=&quot;IIO&quot;),
		&quot;HNR&quot;,
		TEXT(ASI_TH_CRM_Payment_Request__r.ASI_CRM_CN_OutletWS__r.ASI_CRM_SG_Category_Code_13__c)
	) 
	,&quot;&quot;
)</formula>
