    <webLinks>
        <fullName>ASI_Forecast_Apply_to_Forecast</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Apply to Forecast</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/41.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/41.0/apex.js&quot;)}


var groupId = &quot;{!ASI_MFM_Item_Group__c.Id}&quot;;
var params = {itemGroupId:groupId};
var callback = { onSuccess: function(result){
var chMap = JSON.parse(result);
var chSet = chMap.mailChCode;
var chSubmit = chMap.submittedChCode; 
var chFinal = chMap.finalChCode;

console.log(chSet);
if(chSet.length == 0) {
  alert(&quot;No forecast to apply or already apply&quot;);
  
} else if(chSubmit.length&gt;0 || chFinal.length&gt;0) {
  var subCh = &apos;&apos;;
  var finCh = &apos;&apos;;
  for(var i=0; i&lt;chSubmit.length; i++) {
    subCh += chSubmit[i] + &apos;,&apos;;
  }
  for(var i=0; i&lt;chFinal.length; i++) {
    finCh += chFinal[i] + &apos;,&apos;;
  }
  var msg = &apos;Following Channel is in Final or Submitted and auto reset to Draft. \n&apos;;
  alert(msg+&apos;Submitted : \n&apos;+subCh+&apos;\nFinal : \n&apos;+finCh);
} else {
  alert(&quot;Apply to forecast success&quot;);
}
}, onFailure: function(error){} };

sforce.apex.execute(&quot;ASI_CRM_TW_Forecast_Create&quot;, &quot;applyNewItemToCurrentForecast&quot;, params, callback);</url>
    </webLinks>
