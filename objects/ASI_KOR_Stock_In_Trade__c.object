    <fields>
        <fullName>ASI_KOR_SYS_Month__c</fullName>
        <externalId>false</externalId>
        <formula>if(TEXT( ASI_KOR_Month__c)=&quot;January&quot;, &quot;01&quot;,
	if(TEXT( ASI_KOR_Month__c)=&quot;February&quot;,&quot;02&quot;,
		if(TEXT( ASI_KOR_Month__c)=&quot;March&quot;,&quot;03&quot;,
		if(TEXT( ASI_KOR_Month__c)=&quot;April&quot;,&quot;04&quot;,
			if(TEXT( ASI_KOR_Month__c)=&quot;May&quot;,&quot;05&quot;,
				if(TEXT( ASI_KOR_Month__c)=&quot;June&quot;,&quot;06&quot;,
					if(TEXT( ASI_KOR_Month__c)=&quot;July&quot;,&quot;07&quot;,
						if(TEXT( ASI_KOR_Month__c)=&quot;August&quot;,&quot;08&quot;,
							if(TEXT( ASI_KOR_Month__c)=&quot;September&quot;,&quot;09&quot;,
								if(TEXT( ASI_KOR_Month__c)=&quot;October&quot;,&quot;10&quot;,
									if(TEXT( ASI_KOR_Month__c)=&quot;November&quot;,&quot;11&quot;,
											if(TEXT( ASI_KOR_Month__c)=&quot;December&quot;,&quot;12&quot;,&quot;XX&quot;))))))))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>SYS_Month</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
