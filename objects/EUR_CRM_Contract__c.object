    <validationRules>
        <fullName>EUR_CRM_Validate_Start_Date</fullName>
        <active>true</active>
        <description>Contract Start Date should not be earlier than the current fiscal year start date
For the following affiliates:
1.DE</description>
        <errorConditionFormula>AND(
 EUR_CRM_Country_Code__c=&apos;DE&apos;,ISPICKVAL(EUR_CRM_Status__c,&apos;Draft&apos;),  
        IF(MONTH(TODAY()) &lt; 7,
           AND(
              YEAR(EUR_CRM_Start_Date__c) &lt;= YEAR(TODAY())-1,
              MONTH(EUR_CRM_Start_Date__c) &lt; 7
           ),
           AND(
              YEAR(EUR_CRM_Start_Date__c) &lt;= YEAR(TODAY()),
              MONTH(EUR_CRM_Start_Date__c) &lt; 7
           )
       )
)</errorConditionFormula>
        <errorDisplayField>EUR_CRM_Start_Date__c</errorDisplayField>
        <errorMessage>Start Date cannot be earlier than current fiscal year start date</errorMessage>
    </validationRules>
