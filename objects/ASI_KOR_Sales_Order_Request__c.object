    <webLinks>
        <fullName>ASI_CRM_SG_Submit_to_PRS_Wholesaler</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Submit to PRS</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>&apos;{!requireScript(&quot;/soap/ajax/30.0/connection.js&quot;)}&apos;
&apos;{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/apex.js&quot;)} &apos; 
sforce.connection.sessionId = &quot;{!$Api.Session_ID}&quot;; 

try {
	var currentHeaderDetails = sforce.connection.query(&quot;Select Id from ASI_KOR_Sales_Order_Transaction__c where ASI_KOR_Sales_Order_Request__c = &apos;{!ASI_KOR_Sales_Order_Request__c.Id}&apos; &quot;); 
	var detailRecords = currentHeaderDetails.getArray(&quot;records&quot;); 

	if (detailRecords == null || detailRecords.length &lt;= 0) { 
		alert(&quot;Please input Sales Order Request lines&quot;); 
	} 
	// 20160711 add back date alert
	else if ({!ASI_KOR_Sales_Order_Request__c.ASI_CRM_SG_Order_Date__c &lt; TODAY()} || {!ASI_KOR_Sales_Order_Request__c.ASI_CRM_Expected_Delivery_Date__c &lt; TODAY()} || {!ASI_KOR_Sales_Order_Request__c.ASI_CRM_Expected_Delivery_Date__c &lt; ASI_KOR_Sales_Order_Request__c.ASI_CRM_SG_Order_Date__c}){ 
		alert(&quot;No back Order Date / Expected Delivery Date is allowed&quot;); 
	} 
	// 20161229 Delivery Date cannot be later than Order Date for more than 2 days
	else if ({!ASI_KOR_Sales_Order_Request__c.ASI_CRM_Expected_Delivery_Date__c - ASI_KOR_Sales_Order_Request__c.ASI_CRM_SG_Order_Date__c &gt; 2}){ 
		alert(&quot;Your expected delivery date is more than 2 days from your order date. Please contact your Sales Manager in charge&quot;); 
	} 
	else { 
		// Vincent 20160623, add cutoff alert
		var ob = new sforce.SObject(&quot;ASI_KOR_Sales_Order_Request__c&quot;); 
		ob.Id = &apos;{!ASI_KOR_Sales_Order_Request__c.Id}&apos;; 
		ob.ASI_CRM_Email_to_PRS__c = true; 
		ob.ASI_CRM_Submit_to_PRS__c = true;     

		// Hugo 2016-07-06, update order date
		var currentTime = new Date();
		if ((currentTime.getHours()== 15 &amp;&amp; currentTime.getMinutes() &gt; 30) || currentTime.getHours()&gt;15) {
			alert(&apos;Gentle Reminder that our cut off time is 3.30pm. Next day delivery will not be fulfilled for your order&apos;);

			if(Date.parse(&apos;{!ASI_KOR_Sales_Order_Request__c.ASI_CRM_SG_Order_Date__c}&apos;) == (new Date()).setHours(0,0,0,0)) {
				currentTime.setDate(currentTime.getDate() + 1);
				ob.ASI_CRM_SG_Order_Date__c = currentTime;
			}
			if({!ASI_KOR_Sales_Order_Request__c.ASI_CRM_Expected_Delivery_Date__c == TODAY()}) {
				alert(&apos;Please choose an Expected Delivery Date to be at least one day later than today&apos;);
			}
		}
		
		//Wilken 20180412, add AR Checking alert
		if ({!ASI_KOR_Sales_Order_Request__c.ASI_CRM_Over_Credit_Limit__c == true}){
			alert(&quot;Please pay immediately to avoid order disruption.&quot;);
		}

		sforce.connection.update([ob]); 
		location.reload(true); 
	}
} 
catch(error) { 
	sforce.debug.log(error.faultcode); 
	sforce.debug.log(error.faultstring); 
}</url>
