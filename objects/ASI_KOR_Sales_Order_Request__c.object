        <fullName>ASI_CRM_SG_Submit_for_Approval</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Submit for Approval</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!requireScript(&quot;/soap/ajax/30.0/connection.js&quot;)} 
&apos;{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/apex.js&quot;)} &apos; 
sforce.connection.sessionId = &quot;{!$Api.Session_ID}&quot;; 

var currentHeaderDetails = sforce.connection.query(&quot;Select Id from ASI_KOR_Sales_Order_Transaction__c where ASI_KOR_Sales_Order_Request__c = &apos;{!ASI_KOR_Sales_Order_Request__c.Id}&apos; &quot;); 
var detailRecords = currentHeaderDetails.getArray(&quot;records&quot;); 

if (detailRecords == null || detailRecords.length &lt;= 0) { 
	alert(&quot;No Sales Order Request lines! You cannot submit for approval&quot;); 
} 
// Patched by Vincent Lam 20160428, check back date for Wholesaler Sales Order
// BEGIN
else if (&apos;{!ASI_KOR_Sales_Order_Request__c.RecordType}&apos; == &apos;ASI CRM SG Wholesaler Sales Order&apos; &amp;&amp;
 (&apos;{!ASI_KOR_Sales_Order_Request__c.ASI_CRM_SG_Order_Date__c &lt; TODAY()}&apos; == &apos;true&apos; || &apos;{!ASI_KOR_Sales_Order_Request__c.ASI_CRM_Expected_Delivery_Date__c &lt; TODAY()}&apos; == &apos;true&apos; || &apos;{!ASI_KOR_Sales_Order_Request__c.ASI_CRM_Expected_Delivery_Date__c &lt; ASI_KOR_Sales_Order_Request__c.ASI_CRM_SG_Order_Date__c}&apos; == &apos;true&apos;)){
	alert(&quot;No back Order Date / Expected Delivery Date is allowed&quot;);
}
// END
else if (&apos;{!ASI_KOR_Sales_Order_Request__c.ASI_KOR_Order_Status__c}&apos;== &apos;Draft&apos; || &apos;{!ASI_KOR_Sales_Order_Request__c.ASI_KOR_Order_Status__c}&apos;== &apos;Rejected&apos; || &apos;{!ASI_KOR_Sales_Order_Request__c.ASI_KOR_Order_Status__c}&apos;== &apos;New&apos;){ 
	try { 
		// Added entry criteria
		if (&apos;{!ASI_KOR_Sales_Order_Request__c.RecordType}&apos; == &apos;ASI CRM SG Wholesaler Sales Order&apos;) {
			var ob = new sforce.SObject(&quot;ASI_KOR_Sales_Order_Request__c&quot;); 
			ob.Id = &apos;{!ASI_KOR_Sales_Order_Request__c.Id}&apos;; 
			ob.ASI_CRM_Sys_Allow_Submit_Approval__c = true; 
			sforce.connection.update([ob]);
		}
		navigateToUrl(&apos;/apex/ASI_SubmitApprovalPage?id={!ASI_KOR_Sales_Order_Request__c.Id}&apos;);
	} 
	catch(error) { 
		sforce.debug.log(error.faultcode); 
		sforce.debug.log(error.faultstring); 
	} 
}</url>
    </webLinks>
    <webLinks>
        <fullName>ASI_CRM_SG_Submit_for_Approval_TBCN</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Submit for Approval</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!requireScript(&quot;/soap/ajax/30.0/connection.js&quot;)} 
&apos;{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/apex.js&quot;)} &apos; 
sforce.connection.sessionId = &quot;{!$Api.Session_ID}&quot;; 

try { 
var currentHeaderDetails = sforce.connection.query(&quot;Select Id from ASI_KOR_Sales_Order_Transaction__c where ASI_KOR_Sales_Order_Request__c = &apos;{!ASI_KOR_Sales_Order_Request__c.Id}&apos; &quot;); 
var detailRecords = currentHeaderDetails.getArray(&quot;records&quot;); 

var hasAttachment = false; 
var queryString = &quot;SELECT Id, ParentId FROM Attachment WHERE ParentId = &apos;{!ASI_KOR_Sales_Order_Request__c.Id}&apos;&quot;; 
var relatedCharts = sforce.connection.query(queryString); 
records = relatedCharts.getArray(&apos;records&apos;); 
if (records.length &gt; 0){ 
    hasAttachment = true; 
} 

if (detailRecords == null || detailRecords.length &lt;= 0) { 
	alert(&quot;No TBCN Request lines! You cannot submit for approval&quot;); 
} 
else if (!hasAttachment){
	alert(&quot;Please upload supporting document before submit for approval&quot;);
}
else if ({!ASI_KOR_Sales_Order_Request__c.ASI_CRM_SG_Order_Date__c &lt; TODAY()} || {!ASI_KOR_Sales_Order_Request__c.ASI_CRM_Return_Date__c &lt; TODAY()} || {!ASI_KOR_Sales_Order_Request__c.ASI_CRM_Return_Date__c &lt; ASI_KOR_Sales_Order_Request__c.ASI_CRM_SG_Order_Date__c}){ 
        alert(&quot;No back Order Date / Return Date is allowed&quot;); 
} 
else if (&apos;{!ASI_KOR_Sales_Order_Request__c.ASI_KOR_Order_Status__c}&apos;== &apos;Draft&apos; || &apos;{!ASI_KOR_Sales_Order_Request__c.ASI_KOR_Order_Status__c}&apos;== &apos;Rejected&apos; || &apos;{!ASI_KOR_Sales_Order_Request__c.ASI_KOR_Order_Status__c}&apos;== &apos;New&apos;){ 
		// Added entry criteria
		var ob = new sforce.SObject(&quot;ASI_KOR_Sales_Order_Request__c&quot;); 
		ob.Id = &apos;{!ASI_KOR_Sales_Order_Request__c.Id}&apos;; 
		ob.ASI_CRM_Sys_Allow_Submit_Approval__c = true; 
		sforce.connection.update([ob]);
		navigateToUrl(&apos;/apex/ASI_SubmitApprovalPage?id={!ASI_KOR_Sales_Order_Request__c.Id}&apos;);
}
	} 
	catch(error) { 
		sforce.debug.log(error.faultcode); 
		sforce.debug.log(error.faultstring); 
	}</url>
    </webLinks>
