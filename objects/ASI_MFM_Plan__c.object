        <url>{!requireScript(&quot;/soap/ajax/30.0/connection.js&quot;)}
sforce.connection.sessionId = &quot;{!$Api.Session_ID}&quot;;

var currentHeader = sforce.connection.query(&quot;Select ASI_MFM_Status__c, Id from ASI_MFM_Plan__c where Id = &apos;{!ASI_MFM_Plan__c.Id}&apos;&quot;);
var currentHeaderDetails = sforce.connection.query(&quot;Select Id, ASI_MFM_Quotation__c from ASI_MFM_Plan_Line_Item__c where ASI_MFM_Plan__c = &apos;{!ASI_MFM_Plan__c.Id}&apos; &quot;);
var currentHeaderAttachments = sforce.connection.query(&quot;Select Id, Name, ASI_MFM_CAP_Category__c from ASI_MFM_CAP_Plan_Attachment__c where ASI_MFM_Plan__c = &apos;{!ASI_MFM_Plan__c.Id}&apos; &quot;);

var records = currentHeader.getArray(&quot;records&quot;);
var detailRecords = currentHeaderDetails.getArray(&quot;records&quot;);
var attachmentRecords = currentHeaderAttachments.getArray(&quot;records&quot;);
var prefixName = &apos;{!ASI_MFM_Plan__c.ASI_MFM_Prefix__c}&apos;;

var isQuotationRequired = false;
var containsProjectStatement = false;
var containsQuotation = false;


if (!String.prototype.contains) {
	String.prototype.contains = function(s) {
		return this.indexOf(s) &gt; -1
	}
}

for (var i = 0; i &lt; detailRecords.length; i++)
{
	if (detailRecords[i].ASI_MFM_Quotation__c == &quot;Y&quot;){
		isQuotationRequired = true;
	}
}

for (var i = 0; i &lt; attachmentRecords.length; i++)
{
	if (attachmentRecords[i].ASI_MFM_CAP_Category__c == &quot;Project Validation Statement&quot;)
	{
		containsProjectStatement = true;
	}

	if (attachmentRecords[i].ASI_MFM_CAP_Category__c == &quot;Quotation&quot;)
	{
		containsQuotation = true;
	}
}


if (detailRecords == null || detailRecords.length &lt;= 0)
{
	alert(&quot;Plan Line Details Not Found&quot;);
}else if (records[0].ASI_MFM_Status__c== &apos;Draft&apos;)
{
	try {
		var rec = new sforce.SObject(&apos;ASI_MFM_Plan__c&apos;);
		rec.Id = &quot;{!ASI_MFM_Plan__c.Id}&quot;;
		rec.ASI_MFM_Sys_Allow_Submit_Approval__c = true;
		result = sforce.connection.update([rec]);
		window.open(&quot;/apex/ASI_SubmitApprovalPage?id={!ASI_MFM_Plan__c.Id}&quot;,&quot;_self&quot;);
	} catch(error) {
		sforce.debug.log(error.faultcode);
		sforce.debug.log(error.faultstring);
	}
}</url>
