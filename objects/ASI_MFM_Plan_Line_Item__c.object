    <validationRules>
        <fullName>ASI_MFM_AC_Mandatory_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>(RecordType.DeveloperName = &apos;ASI_MFM_TW_Plan_Line_Item&apos;) &amp;&amp; ISBLANK( ASI_MFM_A_C_Code__c )</errorConditionFormula>
        <errorDisplayField>ASI_MFM_A_C_Code__c</errorDisplayField>
        <errorMessage>A/C Code is Mandatory for selection</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_AP_AC_Validation</fullName>
        <active>false</active>
        <errorConditionFormula>!ISBLANK(ASI_MFM_A_C_Code__c) &amp;&amp; (!ISBLANK( ASI_MFM_AP_Code__c ) &amp;&amp; ASI_MFM_A_C_Code__c &lt;&gt;  ASI_MFM_AP_Code__r.ASI_MFM_A_C_Code__c )</errorConditionFormula>
        <errorDisplayField>ASI_MFM_A_C_Code__c</errorDisplayField>
        <errorMessage>The A/C Code is NOT the A/C Code of AP Code selected. Please correct</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_CAP_Check_CAPEX_OPEX_Type</fullName>
        <active>true</active>
        <errorConditionFormula>CONTAINS(RecordType.DeveloperName, &apos;ASI_MFM_CAP&apos;)&amp;&amp;
((ISPICKVAL(ASI_MFM_Plan__r.ASI_MFM_Plan_Type__c, &apos;CAPEX&apos;)&amp;&amp;  ASI_MFM_BU_Code__r.ASI_MFM_OPEX__c = TRUE ) || (ISPICKVAL(ASI_MFM_Plan__r.ASI_MFM_Plan_Type__c, &apos;OPEX&apos;)&amp;&amp; ASI_MFM_BU_Code__r.ASI_MFM_OPEX__c = FALSE ))</errorConditionFormula>
        <errorDisplayField>ASI_MFM_BU_Code__c</errorDisplayField>
        <errorMessage>For CAPEX plan, please select a CAPEX BU Code. For OPEX plan, please select an OPEX BU Code.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_CAP_Future_Available_Date</fullName>
        <active>false</active>
        <errorConditionFormula>CONTAINS(RecordType.DeveloperName,&apos;ASI_MFM_CAP&apos;) &amp;&amp; ISPICKVAL( ASI_MFM_Quotation__c , &apos;N&apos;) &amp;&amp;
ASI_MFM_Quotation_Available_Date__c  &lt;=  TODAY() &amp;&amp;  (ISCHANGED(ASI_MFM_Quotation_Available_Date__c) ||  ISNEW() )</errorConditionFormula>
        <errorDisplayField>ASI_MFM_Quotation_Available_Date__c</errorDisplayField>
        <errorMessage>Quotation Available Date must be later than today.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_CAP_Quotation_Date_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>CONTAINS(RecordType.DeveloperName,&apos;ASI_MFM_CAP&apos;)  &amp;&amp;  ISPICKVAL( ASI_MFM_Quotation__c, &apos;N&apos;) &amp;&amp; !ASI_MFM_T_E__c &amp;&amp; ISBLANK( ASI_MFM_Quotation_Available_Date__c)</errorConditionFormula>
        <errorDisplayField>ASI_MFM_Quotation_Available_Date__c</errorDisplayField>
        <errorMessage>You must specify the Quotation Available Date for records which Quotation select N.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_CAP_Quotation_Reason_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>CONTAINS(RecordType.DeveloperName,&apos;ASI_MFM_CAP&apos;) &amp;&amp;  ISPICKVAL( ASI_MFM_Quotation__c, &apos;N&apos;) &amp;&amp; !ASI_MFM_T_E__c  &amp;&amp; ISBLANK(  ASI_MFM_Justification__c)</errorConditionFormula>
        <errorDisplayField>ASI_MFM_Justification__c</errorDisplayField>
        <errorMessage>You must specify the reason in Justification field for records which Quotation select N.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_GF_PLI_SubBrand_Mandatory</fullName>
        <active>true</active>
        <description>Validation to check either Sub-brand or Sub-brand Allocation has to be selected</description>
        <errorConditionFormula>AND
(
 CONTAINS(RecordType.DeveloperName, &quot;ASI_MFM_GF&quot;),
OR
(
  ISBLANK(ASI_MFM_Sub_brand_Code__c )&amp;&amp; ISBLANK( ASI_MFM_Sub_brand_Allocation_Type__c ),
  NOT(ISBLANK(ASI_MFM_Sub_brand_Code__c ))&amp;&amp; NOT(ISBLANK( ASI_MFM_Sub_brand_Allocation_Type__c))
)
)</errorConditionFormula>
        <errorMessage>Please select Sub-brand or Sub-brand Allocation</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_HK_PlanLineCustomerBUValidation</fullName>
        <active>true</active>
        <description>Ensure correct customer (HK or Macau) is selected</description>
        <errorConditionFormula>AND (  
 CONTAINS(RecordType.DeveloperName, &quot;ASI_MFM_HK&quot;),
  TEXT(ASI_MFM_Customer_Name__r.ASI_HK_CRM_Location__c) !=  ASI_MFM_Plan__r.ASI_MFM_Prefix__r.ASI_MFM_Default_BU__r.ASI_MFM_BU_Code__c 
)</errorConditionFormula>
        <errorDisplayField>ASI_MFM_Customer_Name__c</errorDisplayField>
        <errorMessage>Please select Customer of the correct location (Hong Kong/Macau)</errorMessage>
    </validationRules>
