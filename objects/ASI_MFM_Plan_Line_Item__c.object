        <fullName>ASI_MFM_Lock_HK_PLI</fullName>
        <active>true</active>
        <errorConditionFormula>(ASI_MFM_Plan__r.RecordType.DeveloperName = &apos;ASI_MFM_HK_Plan_Read_Only&apos;) &amp;&amp; !CONTAINS($Profile.Name, &apos;System Administrator&apos;) &amp;&amp; (ASI_MFM_Sys_Sync_Time_Milli__c  = PRIORVALUE(ASI_MFM_Sys_Sync_Time_Milli__c))</errorConditionFormula>
