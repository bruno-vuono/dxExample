    <validationRules>
        <fullName>ASI_MFM_TotalAmount_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>(
	!ISBLANK(ASI_MFM_Month_1_Cost__c)  || 
	!ISBLANK(ASI_MFM_Month_2_Cost__c) || 
	!ISBLANK(ASI_MFM_Month_3_Cost__c)|| 
	!ISBLANK(ASI_MFM_Month_4_Cost__c)|| 
	!ISBLANK(ASI_MFM_Month_5_Cost__c)|| 
	!ISBLANK(ASI_MFM_Month_6_Cost__c)|| 
	!ISBLANK(ASI_MFM_Month_7_Cost__c)|| 
	!ISBLANK(ASI_MFM_Month_8_Cost__c)|| 
	!ISBLANK(ASI_MFM_Month_9_Cost__c)|| 
	!ISBLANK(ASI_MFM_Month_10_Cost__c)|| 
	!ISBLANK(ASI_MFM_Month_11_Cost__c)|| 
	!ISBLANK(ASI_MFM_Month_12_Cost__c)
) 
&amp;&amp;  
( 
	ASI_MFM_Total_Cost__c  &lt;&gt;  
		IF( ISBLANK(ASI_MFM_Month_1_Cost__c) ,0, ASI_MFM_Month_1_Cost__c)  + 
		IF( ISBLANK(ASI_MFM_Month_2_Cost__c) ,0, ASI_MFM_Month_2_Cost__c)  + 
		IF( ISBLANK(ASI_MFM_Month_3_Cost__c) ,0, ASI_MFM_Month_3_Cost__c) + 
		IF( ISBLANK(ASI_MFM_Month_4_Cost__c) ,0, ASI_MFM_Month_4_Cost__c) + 
		IF( ISBLANK(ASI_MFM_Month_5_Cost__c) ,0, ASI_MFM_Month_5_Cost__c) + 
		IF( ISBLANK(ASI_MFM_Month_6_Cost__c) ,0, ASI_MFM_Month_6_Cost__c) + 
		IF( ISBLANK(ASI_MFM_Month_7_Cost__c) ,0, ASI_MFM_Month_7_Cost__c) + 
		IF( ISBLANK(ASI_MFM_Month_8_Cost__c) ,0, ASI_MFM_Month_8_Cost__c) + 
		IF( ISBLANK(ASI_MFM_Month_9_Cost__c) ,0, ASI_MFM_Month_9_Cost__c) + 
		IF( ISBLANK(ASI_MFM_Month_10_Cost__c) ,0, ASI_MFM_Month_10_Cost__c) + 
		IF( ISBLANK(ASI_MFM_Month_11_Cost__c) ,0, ASI_MFM_Month_11_Cost__c) + 
		IF( ISBLANK(ASI_MFM_Month_12_Cost__c) ,0, ASI_MFM_Month_12_Cost__c) 
)
&amp;&amp;
(RecordType.DeveloperName = &quot;ASI_MFM_CN_Plan_Line_Item&quot; || RecordType.DeveloperName = &quot;ASI_MFM_GF_Plan_Line_Item&quot;)</errorConditionFormula>
        <errorDisplayField>ASI_MFM_Total_Cost__c</errorDisplayField>
        <errorMessage>Please make sure the sum of spread amount equal to Plan Line Amount</errorMessage>
    </validationRules>
