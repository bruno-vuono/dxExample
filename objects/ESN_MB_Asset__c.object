        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Import Last DAM Assets</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/31.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/31.0/apex.js&quot;)}

var startDate = Date.today().addDays(-1);
var endDate = Date.today();

alert(&quot;startDate:&quot; + startDate + &apos;, endDate:&apos; + endDate);

var scr = &quot;ESN_MB_Rest_DAM_Batch c = new ESN_MB_Rest_DAM_Batch(startDate, endDate); &quot; +
         &quot; Database.executeBatch(c, 5); &quot;;
var result = sforce.apex.executeAnonymous(scr);
 
alert(&quot;The Batch is Running. Please wait 5 minutes.&quot;);</url>
    </webLinks>
