    <rules>
        <fullName>ASI_CRM_CN_PriceAndDiscount_IHBR_Notification</fullName>
        <actions>
            <name>ASI_CRM_CN_PriceAndDiscount_Notification_Email_IHBR</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <formula>ISPICKVAL(ASI_CRM_Status__c, &apos;Completed&apos; ) &amp;&amp;  ASI_CRM_Customer__r.ASI_CRM_CN_Is_IHBR_Outlet__c  == TRUE &amp;&amp; ( RecordType.DeveloperName == &apos;ASI_CRM_CN_Customer_Price_and_Discount_Request_RO&apos; || RecordType.DeveloperName == &apos;ASI_CRM_CN_Customer_Price_and_Discount_Request&apos; )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>ASI_CRM_CN_PriceAndDiscount_Notification</fullName>
        <actions>
            <name>ASI_CRM_CN_PriceAndDiscount_Notification_Email</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <formula>( ISPICKVAL(ASI_CRM_Status__c, &apos;Pending for T1 Price&apos;) || ISPICKVAL(ASI_CRM_Status__c, &apos;Approved&apos; ) ) &amp;&amp; ( RecordType.DeveloperName == &apos;ASI_CRM_CN_Customer_Price_and_Discount_Request_RO&apos; || RecordType.DeveloperName == &apos;ASI_CRM_CN_Repacking_Price_Request_RO&apos; || RecordType.DeveloperName == &apos;ASI_CRM_CN_T1_Price_Adjustment_Request_RO&apos; )</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
