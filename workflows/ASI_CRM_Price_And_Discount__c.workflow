    <rules>
        <fullName>ASI_CRM_CN_FinanceNoti_1</fullName>
        <actions>
            <name>ASI_CRM_CN_PriceAndDiscount_NewT1Price_Notification_Email</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <formula>IF( ( RecordType.DeveloperName == &quot;ASI_CRM_CN_NPL_T1_Price_Request&quot; || RecordType.DeveloperName == &quot;ASI_CRM_CN_NPL_T1_Price_Request_RO&quot; ) &amp;&amp;  ISPICKVAL( ASI_CRM_Status__c, &quot;Pending for PRCSW to PRC Price&quot;), TRUE, FALSE)</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>ASI_CRM_CN_FinanceNoti_2</fullName>
